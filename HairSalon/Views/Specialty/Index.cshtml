<h1>EpiCutUs Salon</h1>


<h3>Stylist Specialties: </h3>

@if (Model.Count != 0)
{
    <ul>
    @foreach (var specialty in Model)
    {
        <li><a href='/specialty/@specialty.GetId()'>@specialty.GetSpecialty()</a></li>
    }
    </ul>
}

<h4><a href="/specialty/new">Add new stylist specialty</a></h4>
<form action="/specialty/deleteall" method="delete">
  <button type="submit">Delete specialty</button>
</form>
<h4><a href="/stylists">View all stylists</a></h4>
<h4><a href="/clients">View all clients</a></h4>
<h4><a href="/">Salon Home</a></h4>